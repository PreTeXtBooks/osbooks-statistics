          <row><cell>20</cell><cell>19</cell><cell>21</cell><cell>20</cell><cell>18</cell><cell>20</cell><cell>20</cell><cell>19</cell><cell>18</cell><cell>19</cell></row>
          <row><cell>Chicken Breasts</cell><cell>17</cell><cell>19</cell><cell>17</cell><cell>21</cell><cell>17</cell><cell>21</cell><cell>18</cell><cell>21</cell><cell>19</cell><cell>21</cell></row>
          <row><cell>20</cell><cell>17</cell><cell>20</cell><cell>18</cell><cell>19</cell><cell>20</cell><cell>20</cell><cell>17</cell><cell>21</cell><cell>20</cell></row>
        </tabular>
